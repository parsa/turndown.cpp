@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Add our cmake directory to module path for FindGumbo.cmake
list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

find_dependency(Gumbo)

include("${CMAKE_CURRENT_LIST_DIR}/turndown_cpp_targets.cmake")

# Provide the turndown_cpp::gumbo alias for consistency
if(NOT TARGET turndown_cpp::gumbo)
    add_library(turndown_cpp::gumbo ALIAS Gumbo::Gumbo)
endif()
